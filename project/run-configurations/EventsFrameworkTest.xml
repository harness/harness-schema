<configuration name="EventsFrameworkTest"
               type="BlazeCommandRunConfigurationType"
               factoryName="Bazel Command">
    <blaze-settings handler-id="BlazeJavaRunConfigurationHandlerProvider"
                    kind="java_binary"
                    debug_port="6019"
                    blaze-command="run"
                    keep-in-sync="true">
        <blaze-target>//953-events-api:events_framework_test</blaze-target>
        <blaze-user-exe-flag>server</blaze-user-exe-flag>
        <blaze-user-exe-flag>$PROJECT_DIR$/../953-events-api/src/test/config.yml</blaze-user-exe-flag>
    </blaze-settings>
    <method v="2">
        <option name="Blaze.BeforeRunTask" enabled="true"/>
    </method>
</configuration>
