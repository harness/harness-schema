        <blaze-target>//pipeline-service/service:module</blaze-target>
        <blaze-user-exe-flag>server</blaze-user-exe-flag>
        <blaze-user-exe-flag>$PROJECT_DIR$/../pipeline-service/config/config.yml</blaze-user-exe-flag>
    </blaze-settings>
    <method v="2">
        <option name="Blaze.BeforeRunTask" enabled="true"/>
    </method>
</configuration>