<configuration name="DashboardApp"
               type="BlazeCommandRunConfigurationType"
               factoryName="Bazel Command">
<blaze-settings handler-id="BlazeJavaRunConfigurationHandlerProvider"
                kind="java_binary"
                debug_port="6021"
                blaze-command="run"
                keep-in-sync="true">
    <blaze-target>//290-dashboard-service:module</blaze-target>
    <blaze-user-exe-flag>server</blaze-user-exe-flag>
    <blaze-user-exe-flag>$PROJECT_DIR$/../290-dashboard-service/config.yml</blaze-user-exe-flag>
</blaze-settings>
<method v="2">
    <option name="Blaze.BeforeRunTask" enabled="true"/>
</method>
</configuration>
