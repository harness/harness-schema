<configuration name="DelegateApp"
               type="BlazeCommandRunConfigurationType"
               factoryName="Bazel Command">
    <blaze-settings handler-id="BlazeJavaRunConfigurationHandlerProvider"
                    kind="java_binary"
                    debug_port="6008"
                    blaze-command="run"
                    keep-in-sync="true">
        <blaze-target>//260-delegate:execute</blaze-target>
        <blaze-user-exe-flag>$PROJECT_DIR$/../260-delegate/config-delegate.yml</blaze-user-exe-flag>
    </blaze-settings>
    <method v="2">
        <option name="Blaze.BeforeRunTask" enabled="true"/>
    </method>
</configuration>
