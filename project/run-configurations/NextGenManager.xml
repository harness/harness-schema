<configuration name="NextGen Manager"
               type="BlazeCommandRunConfigurationType"
               factoryName="Bazel Command">
    <blaze-settings handler-id="BlazeCommandGenericRunConfigurationHandlerProvider"
                    blaze-command="run"
                    kind="java_library">
        <blaze-target>//120-ng-manager:module</blaze-target>
        <blaze-user-exe-flag>server</blaze-user-exe-flag>
        <blaze-user-exe-flag>$PROJECT_DIR$/../120-ng-manager/config.yml</blaze-user-exe-flag>
    </blaze-settings>
    <method v="2">
        <option name="Blaze.BeforeRunTask" enabled="true"/>
    </method>
</configuration>